#zsh + slimzsh
sudo apt install git
git clone --recursive https://github.com/changs/slimzsh.git ~/.slimzsh
sudo apt install zsh
#save in $home 
wget -O .zshrc -P ./home/tomasz https://raw.githubusercontent.com/tomasszz/myMintConfig/main/zshrc 
#as .zshrc

#Aura theme terminal
#save in $home 
wget -P ./home/tomasz https://raw.githubusercontent.com/daltonmenezes/aura-theme/main/packages/gnome-terminal/aura-theme.dconf
dconf load /org/gnome/terminal/legacy/profiles:/:b1dcc9dd-5262-4d8d-a863-c897e6d979b9/ < ~/aura-theme.dconf

#Aura theme walpapers
https://github.com/daltonmenezes/aura-theme/tree/main/packages/wallpapers

#snap store
sudo rm /etc/apt/preferences.d/nosnap.pref
sudo apt update
sudo apt install snapd
sudo snap instll snap-store

